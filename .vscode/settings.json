{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": [
    "source.addMissingImports", 
    "source.fixAll.eslint"
  ],
  "files.eol": "\n",
  "prettier.endOfLine": "lf",
  "[typescript]": {
    "editor.tabSize": 2
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  }
}
